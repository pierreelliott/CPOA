\par \pard\plain \s0\ql\nowidctlpar\ltrpar{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\kerning1\dbch\af11\langfe2052\dbch\af13\afs24\alang1081\cf0\loch\f3\fs24\lang1036\li0\ri0\lin0\rin0\fi0{\dbch\af17\rtlch \ltrch\loch\loch\f8
